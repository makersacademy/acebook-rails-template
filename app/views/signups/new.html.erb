<h1>Welcome to Catflap</h1>

<%= form_with scope: :signup, url: signups_path, local: true do |form| %>
  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </p>
 
  <p>
    <%= form.label :password %><br>
    <%= form.text_field :password %>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
<% end %>