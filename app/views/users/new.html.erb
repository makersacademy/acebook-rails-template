<li><%= link_to 'Homepage', '/posts', method: :get%></li>
<li class="right_links"><%= link_to 'Login', '/login', method: :get%></li>

<br>

<h1>Sign Up</h1>
<%= form_for @user do |f|%>
  <%= f.label :username%><br>
  <%= f.text_field :username%><br>
  <%= f.label :password%><br>
  <%= f.password_field :password%><br>
  <%= f.submit %>
<%end%>