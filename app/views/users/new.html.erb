<%= form_for :user, url: '/users' do |f| %>
<br>
<div class ="form-group">
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
</div>

<div class = "form-group">
  <%= f.label :email %>
  <%= f.text_field :email %>
<br>
</div>

<div class = "form=group">
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
</div>

  <%= f.submit "Submit" %>

<% end %>
